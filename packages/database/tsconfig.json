{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "noEmit": true,
    "outDir": "./dist",
    "rootDir": "."
  },
  "include": ["index.ts", "prisma/seed.ts", "generated/**/*.ts"],
  "exclude": ["node_modules"]
}
